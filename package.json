{
  "name": "openagent-monorepo",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "build:extension": "pnpm --dir apps/extension compile",
    "build:backend": "pnpm --dir apps/backend build",
    "build:shared": "pnpm --dir packages/shared build",
    "build": "pnpm build:shared && pnpm build:extension && pnpm build:backend"
  }
}
